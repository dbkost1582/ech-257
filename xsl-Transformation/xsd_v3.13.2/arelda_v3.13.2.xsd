<?xml version="1.0" encoding="UTF-8"?>
<!-- ****************************************************************************************-->
<!-- XSD für GEVER und FILES Ablieferungen (Projekt ARELDA)                                  -->
<!-- Geändert für Version 3.5.0                                                            -->
<!-- * Nur noch 1 <unstructured> für die gesamte Ablieferung                                 -->
<!-- * Anpassung targetNamespace http://bar.admin.ch/xmlns/arelda/v3                         -->
<!-- * Nur das Hauptschema hat targetNamespace und version konfiguriert,                     -->
<!--   kein targetNamespace für die übrigen Schemas.                                         -->
<!-- * Dokumentation für Typ datumTyp3 hinzugefügt.                                          -->
<!-- * Länge und Dokumentation für Attribute in Entität Datei.                               -->
<!-- * Attribut pruefalgorithmus (Datei) als Enumeration.                                    -->
<!-- * Attribut vorgangstyp (ArchivischerVorgang) als Enumeration.                           -->
<!-- * Id für die Entitäten neu als Attribut definiert.                                      -->
<!-- * Dossier zeigt auf Ordner im Inhaltsverzeichnis (Ersatz für dossierPfad)               -->
<!-- * Id für die Entitäten wird auf Eindeutigkeit geprüft (bei xs:string).                  -->
<!--   xs:string für Ordnungssystemposition und Dokument                                     -->
<!--   xs:ID für Dossier und Datei                                                           -->
<!-- * Sequenz in Dossier und Dokument als Typ xs:nonNegativeInteger                         -->
<!-- * Schutzfristen -> Schutzfristenkategorie + Schutzfrist für                             -->
<!--   Ablieferung, Ordnungssystemposition und Dossier                                       -->
<!-- * Neues Element für ArchivischeNotiz für alle Entitäten ausser ArchivischerVorgang      -->
<!-- * Metadaten für Datei als Element/Attribut für Wert u. Name                             -->
<!-- * Ablieferungsart SIARD entfernen (identisch mit FILES)                                 -->
<!--   Ablieferungstyp Enumeration geändert, Kommentare noch nicht angepasst                 -->
<!-- Geändert für Version 3.6.0                                                              -->
<!-- * Reihenfolge für Element version (Entität Paket) vor inhaltsverzeichnis                -->
<!-- * Reihenfolge für Element ordner (Entität Ordner) vor datei                             -->
<!-- * Element metadaten (Entität Datei) nach eigenschaft umbenennen                         -->
<!-- * Datentyp falsch für folgende Elemente (datumtyp1 statt datumtyp2)                     -->
<!--   Element entstehungszeitraum (Entität Ablieferung)                                     -->
<!--   Element anwendungszeitraum (Entität Ordnungssystem)                                   -->
<!--   Element existenzzeitraum (Entität Provenienz)                                         -->
<!-- * Datentyp falsch für folgendes Element (datumtyp2 statt datumtyp4)                     -->
<!--   Element datum (Entität ArchivischerVorgang)                                           -->
<!--   Definition von datumtyp4 mit regular expression (hat noch gefehlt).                   -->
<!-- * Id für Entität Datei neu als Attribut definiert.                                      -->
<!-- * Attribut sequenz für Dossier und Dokument entfernt.                                   -->
<!-- * Kein Link zwischen Dossier und Ordner im Inhaltsverzeichnis via xs:ID.                -->
<!-- * Id für die Entitäten wird auf Eindeutigkeit geprüft (bei xs:string).                  -->
<!--   xs:string für Ordnungssystemposition, Dossier und Dokument                            -->
<!--   xs:ID für Datei                                                                       -->
<!-- * Element datei unter Dokument und UnstrukturierterAnhang heisst neu dateiRef.          -->
<!-- * Entität Dossier hat für FILES Ablieferungen ein optionales Element dateiRef.          -->
<!-- * Für mandatory Elemente muss <xs:minLength value="1"/> definiert sein.                 -->
<!-- * Entität ArchivischerVorgang kann im AIP 0..n mal vorkommen.                           -->
<!-- Geändert für Version 3.7.0                                                              -->
<!-- * Kardinalität von ArchivischerVorgang auf 1..n geändert.                               -->
<!-- * XML Beispiele für Files/Gever AIP aktualisiert.                                       -->
<!-- * Elemente dossierPfad und ordnungsSystemPositionId aus dossier.xsd entfernt.           -->
<!-- * Anpassung namespace und targetNamespace http://bar.admin.ch/arelda/v3                 -->
<!-- Geändert für Version 3.8.0                                                              -->
<!-- * Element systemBeschreibung angepasst (Entität Provenienz)                             -->
<!-- * Element verwandteSysteme (Entität Provenienz)                             			 -->
<!-- * Element archivierungsmodusLoeschvorschriften(Entität Provenienz)                      -->
<!-- * Element bemerkung angepasst (Entität Provenienz)                      				 -->
<!-- * Element inhalt (Entität Dossier)                      				 				 -->
<!-- * Element formInhalt (Entität Dossier)                      				 			 -->
<!-- * Element umfang (Entität Dossier)                      				 				 -->
<!-- * Element bemerkung angepasst (Entität Dossier)                      				 	 -->
<!-- Geändert für Version 3.9.0                                                              -->
<!-- * Element globaleAIPId  (Entität Paket)                            					 -->
<!-- * Element lokaleAIPId  (Entität Paket)                            					 	 -->
<!-- Geändert für Version 3.9.1                                                              -->
<!-- * Neue Typen Text1, Text2, Text3 und Text4 mit Längeneinschränkungen gem. DD			 -->
<!-- * Verwendung von java.langBoolean statt simple type boolean							 -->
<!-- Geändert für Version 3.9.2                                                              -->
<!-- * Element Originalname, Prüfalgoritmus, Prüfsumme optional (Entität Datei)				 -->
<!-- * Element Federführende Organisationseinheit  optional (Entität OSP)				 	 -->
<!-- Geändert für Version 3.9.3                                                              -->
<!-- * Element eigenschaftDatei (Entität Datei)				 								 -->
<!-- Geändert für Version 3.10.0                                                             -->
<!-- * <archivischeNotiz> (notizDatum, notizErfasser, notizBeschreibung) für DIP GEVER und   -->
<!--   FILES auf "optional" gesetzt.														 -->
<!-- * Entität Paket: Änderungen bei der Relevanz der Attributen "globaleAIPId",			 -->
<!--   "fruehereLokaleAIPId" und "version".													 -->
<!-- * Entität Paket: Das Attribut "speicherdatum" wurde gelöscht.  						 -->
<!-- * uniqueDateiRef Constraint korrigiert													 -->
<!-- Geändert für Version 3.10.1                                                             -->
<!-- * <archivischeNotiz> (notizDatum, notizErfasser, notizBeschreibung) für AIP GEVER und   -->
<!--   FILES auf entfernt														 			 -->
<!-- Geändert für Version 3.11.0                                                             -->
<!-- * Verwenung von Text1-Text4 statt xs:string											 -->
<!-- * Added complex type 'comparable' that is mapped to java.lang.Comparable				 -->
<!-- Geändert für Version 3.12.0                                                             -->
<!-- * Version zum Namespace hinzugefügt													 -->
<!-- * "archivischerVorgang" zu PaketSIP hinzugefügt										 -->
<!-- Geändert für Version 3.13.0                                                             -->
<!-- * referenzUebernahmedossier zu PaketSIP hinzugefügt									 -->
<!-- * "archivischerVorgang" optional														 -->
<!-- Geändert für Version 3.13.1                                                             -->
<!-- * Anpassungen Dokumentation															 -->
<!-- Geändert für Version 3.13.2                                                             -->
<!-- * java.lang.Boolean ersetzt durch primitive type boolean								 -->
<!-- ****************************************************************************************-->
<xs:schema xmlns:mv="http://www.ech.ch/xmlns/minorversion/v1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:arelda="http://bar.admin.ch/arelda/v3.13.2" xmlns="http://bar.admin.ch/arelda/v3.13.2" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" targetNamespace="http://bar.admin.ch/arelda/v3.13.2" elementFormDefault="qualified" attributeFormDefault="unqualified" version="3" mv:minorVersion=".13.2" ecore:package="ch.admin.bar.arelda.v3" ecore:nsPrefix="v3">
	<xs:include schemaLocation="ablieferung.xsd"/>
	<xs:include schemaLocation="paket.xsd"/>
	<xs:include schemaLocation="provenienz.xsd"/>
	<xs:include schemaLocation="ordnungssystem.xsd"/>
	<xs:include schemaLocation="ordnungssystemposition.xsd"/>
	<xs:include schemaLocation="dossier.xsd"/>
	<xs:include schemaLocation="dokument.xsd"/>
	<xs:include schemaLocation="ordner.xsd"/>
	<xs:include schemaLocation="datei.xsd"/>
	<xs:include schemaLocation="archivischerVorgang.xsd"/>
	<xs:include schemaLocation="archivischeNotiz.xsd"/>
	<xs:element name="paket" type="paket">
		<!--
		<xs:key name="ordnungssystempositionIdKey">
			<xs:annotation>
				<xs:documentation>Das Element id in der Entität Ordnungssystemposition muss eindeutig sein.</xs:documentation>
			</xs:annotation>
			<xs:selector xpath=".//arelda:ordnungssystemposition"/>
			<xs:field xpath="@id"/>
		</xs:key>
		-->
		<xs:key name="dossierIdKey">
			<xs:annotation>
				<xs:documentation>Das Element id in der Entität Dossier muss eindeutig sein.</xs:documentation>
			</xs:annotation>
			<xs:selector xpath=".//arelda:dossier"/>
			<xs:field xpath="@id"/>
		</xs:key>
		<xs:key name="dokumentIdKey">
			<xs:annotation>
				<xs:documentation>Das Element id in der Entität Dokument muss eindeutig sein.</xs:documentation>
			</xs:annotation>
			<xs:selector xpath=".//arelda:dokument"/>
			<xs:field xpath="@id"/>
		</xs:key>
		<xs:key name="archivischeNotizIdKey">
			<xs:annotation>
				<xs:documentation>Das Element id in der Entität ArchivischeNotiz muss eindeutig sein.</xs:documentation>
			</xs:annotation>
			<xs:selector xpath=".//arelda:archivischeNotiz"/>
			<xs:field xpath="@id"/>
		</xs:key>
		<xs:unique name="uniqueDateiRef">
			<xs:annotation>
				<xs:documentation>Eine Datei darf nur einmal referenziert werden.</xs:documentation>
			</xs:annotation>
			<xs:selector xpath=".//arelda:dateiRef"/>
			<xs:field xpath="."/>
		</xs:unique>
	</xs:element>
	<xs:complexType name="paket" abstract="true">
		<xs:sequence>
			<xs:element name="paketTyp" type="paketTyp"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="paketSIP">
		<xs:complexContent>
			<xs:extension base="paket">
				<xs:sequence>
					<xs:element name="inhaltsverzeichnis" type="inhaltsverzeichnisSIP"/>
					<xs:element name="ablieferung" type="ablieferungSIP"/>
					<xs:element name="archivischerVorgang" type="archivischerVorgang" minOccurs="0" maxOccurs="unbounded" ecore:name="archivischeVorgaenge"/>
					<xs:element name="referenzUebernahmedossier" type="referenzUebernahmedossier" minOccurs="0"/>
					<xs:element name="archivischeNotiz" type="archivischeNotiz" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="paketAIP">
		<xs:complexContent>
			<xs:extension base="paket">
				<xs:sequence>
					<xs:element name="globaleAIPId" type="globaleAIPId"/>
					<xs:element name="lokaleAIPId" type="lokaleAIPId"/>
					<xs:element name="fruehereLokaleAIPId" type="fruehereLokaleAIPId" minOccurs="0"/>
					<xs:element name="version" type="version" minOccurs="1"/>
					<xs:element name="inhaltsverzeichnis" type="inhaltsverzeichnisAIP"/>
					<xs:element name="ablieferung" type="ablieferungAIP"/>
					<xs:element name="archivischerVorgang" type="archivischerVorgang" minOccurs="0" maxOccurs="unbounded" ecore:name="archivischeVorgaenge"/>
					<xs:element name="referenzUebernahmedossier" type="referenzUebernahmedossier" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="paketDIP">
		<xs:complexContent>
			<xs:extension base="paket">
				<xs:sequence>
					<xs:element name="globaleAIPId" type="globaleAIPId"/>
					<xs:element name="lokaleAIPId" type="lokaleAIPId"/>
					<xs:element name="fruehereLokaleAIPId" type="fruehereLokaleAIPId" minOccurs="0"/>
					<xs:element name="inhaltsverzeichnis" type="inhaltsverzeichnisDIP"/>
					<xs:element name="ablieferung" type="ablieferungDIP"/>
					<xs:element name="archivischeNotiz" type="archivischeNotiz" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="archivischerVorgang">
		<xs:sequence>
			<xs:element name="vorgangstyp" type="vorgangstyp"/>
			<xs:element name="beschreibung" type="beschreibung"/>
			<xs:element name="datum" type="zeitraum">
				<xs:annotation>
					<xs:documentation>Zeitpunkt, zu welchem der archivische Vorgang durchgeführt wurde.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bearbeiter" type="bearbeiter"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ablieferung" abstract="true">
		<xs:sequence>
			<xs:element name="ablieferungstyp" type="ablieferungstyp"/>
			<xs:element name="ablieferndeStelle" type="ablieferndeStelle"/>
			<xs:element name="entstehungszeitraum" type="historischerZeitraum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Der Entstehungszeitraum der Ablieferung wird aus dem ältesten Entstehungsdatum und dem jüngsten Datum der Dokumente oder Dateien ermittelt, die der Ablieferung zugeordneten  sind. Der Zeitraum kann auch geschätzt sein.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ablieferungsteile" type="ablieferungsteile" minOccurs="0"/>
			<xs:element name="bemerkung" type="bemerkungAblieferung" minOccurs="0"/>
			<xs:element name="unstrukturierterAnhang" type="unstrukturierterAnhang" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ablieferungSIP" abstract="true">
		<xs:complexContent>
			<xs:extension base="ablieferung">
				<xs:sequence>
					<xs:element name="ablieferungsnummer" type="ablieferungsnummer" minOccurs="0"/>
					<xs:element name="angebotsnummer" type="angebotsnummer" minOccurs="0"/>
					<xs:element name="referenzBewertungsentscheid" type="referenzBewertungsentscheid" minOccurs="0"/>
					<xs:element name="referenzSchutzfristenFormular" type="referenzSchutzfristenFormular" minOccurs="0"/>
					<xs:element name="schutzfristenkategorie" type="schutzfristenkategorieAblieferung" minOccurs="0"/>
					<xs:element name="schutzfrist" type="schutzfristAblieferung" minOccurs="0"/>
					<xs:element name="archivischeNotiz" type="archivischeNotiz" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ablieferungGeverSIP">
		<xs:complexContent>
			<xs:extension base="ablieferungSIP">
				<xs:sequence>
					<xs:element name="provenienz" type="provenienzGeverSIP"/>
					<xs:element name="ordnungssystem" type="ordnungssystemGeverSIP"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ablieferungFilesSIP">
		<xs:complexContent>
			<xs:extension base="ablieferungSIP">
				<xs:sequence>
					<xs:element name="provenienz" type="provenienzFilesSIP"/>
					<xs:element name="ordnungssystem" type="ordnungssystemFilesSIP"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ablieferungAIP" abstract="true">
		<xs:complexContent>
			<xs:extension base="ablieferung">
				<xs:sequence>
					<xs:element name="ablieferungsnummer" type="ablieferungsnummer"/>
					<xs:element name="angebotsnummer" type="angebotsnummer" minOccurs="0"/>
					<xs:element name="referenzBewertungsentscheid" type="referenzBewertungsentscheid" minOccurs="0"/>
					<xs:element name="referenzSchutzfristenFormular" type="referenzSchutzfristenFormular" minOccurs="0"/>
					<xs:element name="schutzfristenkategorie" type="schutzfristenkategorieAblieferung" minOccurs="0"/>
					<xs:element name="schutzfrist" type="schutzfristAblieferung" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ablieferungGeverAIP">
		<xs:complexContent>
			<xs:extension base="ablieferungAIP">
				<xs:sequence>
					<xs:element name="provenienz" type="provenienzGeverAIP"/>
					<xs:element name="ordnungssystem" type="ordnungssystemGeverAIP"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ablieferungFilesAIP">
		<xs:complexContent>
			<xs:extension base="ablieferungAIP">
				<xs:sequence>
					<xs:element name="provenienz" type="provenienzFilesAIP"/>
					<xs:element name="ordnungssystem" type="ordnungssystemFilesAIP"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ablieferungDIP" abstract="true">
		<xs:complexContent>
			<xs:extension base="ablieferung">
				<xs:sequence>
					<xs:element name="ablieferungsnummer" type="ablieferungsnummer"/>
					<xs:element name="archivischeNotiz" type="archivischeNotiz" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ablieferungGeverDIP">
		<xs:complexContent>
			<xs:extension base="ablieferungDIP">
				<xs:sequence>
					<xs:element name="provenienz" type="provenienzGeverDIP"/>
					<xs:element name="ordnungssystem" type="ordnungssystemGeverDIP"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ablieferungFilesDIP">
		<xs:complexContent>
			<xs:extension base="ablieferungDIP">
				<xs:sequence>
					<xs:element name="provenienz" type="provenienzFilesDIP"/>
					<xs:element name="ordnungssystem" type="ordnungssystemFilesDIP"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="provenienz" abstract="true">
		<xs:sequence>
			<xs:element name="aktenbildnerName" type="aktenbildnerName"/>
			<xs:element name="systemName" type="systemName" minOccurs="0"/>
			<xs:element name="systemBeschreibung" type="systemBeschreibung" minOccurs="0"/>
			<xs:element name="existenzzeitraum" type="historischerZeitraum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Zeitraum der Existenz des Aktenbildners.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="geschichteAktenbildner" type="geschichteAktenbildner" minOccurs="0"/>
			<xs:element name="bemerkung" type="bemerkungProvenienz" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="provenienzGever" abstract="true">
		<xs:complexContent>
			<xs:extension base="provenienz">
				<xs:sequence>
					<xs:element name="registratur" type="registratur"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="provenienzGeverSIP">
		<xs:complexContent>
			<xs:extension base="provenienzGever">
				<xs:sequence>
					<xs:element name="archivischeNotiz" type="archivischeNotiz" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="provenienzGeverAIP">
		<xs:complexContent>
			<xs:extension base="provenienzGever">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="provenienzGeverDIP">
		<xs:complexContent>
			<xs:extension base="provenienzGever">
				<xs:sequence>
					<xs:element name="archivischeNotiz" type="archivischeNotiz" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="provenienzFiles" abstract="true">
		<xs:complexContent>
			<xs:extension base="provenienz">
				<xs:sequence>
					<xs:element name="registratur" type="registratur" minOccurs="0"/>
