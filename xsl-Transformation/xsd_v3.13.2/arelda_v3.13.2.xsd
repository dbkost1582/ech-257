	</xs:complexType>
	<xs:complexType name="provenienzFilesSIP">
		<xs:complexContent>
			<xs:extension base="provenienzFiles">
				<xs:sequence>
					<xs:element name="archivischeNotiz" type="archivischeNotiz" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="provenienzFilesAIP">
		<xs:complexContent>
			<xs:extension base="provenienzFiles">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="provenienzFilesDIP">
		<xs:complexContent>
			<xs:extension base="provenienzFiles">
				<xs:sequence>
					<xs:element name="archivischeNotiz" type="archivischeNotiz" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ordnungssystem" abstract="true">
		<xs:sequence>
			<xs:element name="generation" type="generation" minOccurs="0"/>
			<xs:element name="anwendungszeitraum" type="historischerZeitraum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Zeitraum, seit/während dem das Ordnungssystem bei der aktenbildenden Stelle eingesetzt wird/wurde.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mitbenutzung" type="mitbenutzung" minOccurs="0"/>
			<xs:element name="bemerkung" type="bemerkungOrdnungssysstem" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ordnungssystemGever" abstract="true">
		<xs:complexContent>
			<xs:extension base="ordnungssystem">
				<xs:sequence>
					<xs:element name="name" type="nameOrdnungssystem"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ordnungssystemGeverSIP">
		<xs:complexContent>
			<xs:extension base="ordnungssystemGever">
				<xs:sequence>
					<xs:element name="ordnungssystemposition" type="ordnungssystempositionGeverSIP" minOccurs="1" maxOccurs="unbounded" ecore:name="ordnungssystempositionen"/>
					<xs:element name="archivischeNotiz" type="archivischeNotiz" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ordnungssystemGeverAIP">
		<xs:complexContent>
			<xs:extension base="ordnungssystemGever">
				<xs:sequence>
					<xs:element name="ordnungssystemposition" type="ordnungssystempositionGeverAIP" minOccurs="1" maxOccurs="unbounded" ecore:name="ordnungssystempositionen"/>
				</xs:sequence>
			</xs:extension>
